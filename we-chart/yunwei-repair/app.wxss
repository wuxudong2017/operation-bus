/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  /* padding: 200rpx 0; */
  box-sizing: border-box;
} 
page{
  width: 100%;
  height: 100%;
  background-color: #EEEEEE;
}
view{
  box-sizing: border-box;
  font-size: 12px;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 14px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-shijian:before {
  content: "\e66d";
}

.icon-dingwei:before {
  content: "\e618";
}

/* 
列表显示 */
.list{
  padding: 60rpx 40rpx;
}
.list .list-item{
  padding: 20rpx 40rpx;
  background: #ffffff;
  border-radius: 5px;
  position: relative;
  margin-bottom: 40rpx;
}
.list .list-item .box{
  position: relative;
  overflow: hidden;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #DDDDDD
}
 .list .list-item .box image{
  width: 80rpx;
  float: left;
  height: 80rpx;
}
.list .list-item .box .content{
  padding-left: 40rpx;
  float: left;
}
.list .list-item .box .content .type{
  color: #000;
  font-size: 14px;
}
.list  .list-item .box .content .time{
  padding-top: 10rpx;
  color: #DDDDDD;
  font-size: 12px;
}
.list .list-item .position{
  padding-top: 20rpx;
  color: #AFAFAF;
  padding-left: 40rpx;
}
.list .list-item  .list-id{
  line-height: 40rpx;
  padding-bottom: 8rpx;
  font-size: 12px;
  color: #666;
  text-align: right
}


